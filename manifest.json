{
  "manifest_version": 2,
  "name": "BUBT Ramadan Routine Fixer",
  "version": "1.1.0",
  "description": "Modify time slots of annex routine",
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://annex.bubt.edu.bd/*"],
      "include_globs": ["*routine*"],
      "js": ["content.js"]
    }
  ]
}
